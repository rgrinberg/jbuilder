(jbuild_version 1)

(executable
 ((name cram)
  (libraries (test_common jbuilder))))

(ocamllex (cram))

(alias
 ((name   runtest)
  (deps   ((package jbuilder)
           (files_recursively_in test-cases/redirections)))
  (action
   (chdir test-cases/redirections
    (progn
     (run ${exe:cram.exe} run.t)
     (diff? run.t run.t.corrected))))))

