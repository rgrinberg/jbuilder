(jbuild_version 1)

(library
 ((name foo)
  (modules (foo))
  (wrapped false)
  (public_name foo)))

(library
 ((name foo_byte)
  (modules (foo_byte))
  (modes (byte))
  (public_name foo.byte)))

(alias
 ((name package-listing)
  (deps (_doc/_html/index.html))
  (action (echo "${read:_doc/_html/index.html}"))))

(alias
 ((name foo-module-listing)
  (deps (_doc/_html/foo/index.html))
  (action (echo "${read:_doc/_html/foo/index.html}"))))
